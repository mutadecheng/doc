
:: 添加注册表登录启动程序

cd /d %~dp0
cd /d ../Mcc.RemoteControl.NetFx
set appPath=%cd%

reg add HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Run /v Mcc_RemoteControl /t reg_sz /d "%appPath%\Mcc.RemoteControl.NetFx.exe" /f

pause

